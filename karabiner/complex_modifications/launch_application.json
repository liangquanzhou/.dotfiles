{
    "title": "Launch Applications",
    "rules": [
        {
            "description": "Open Finder",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                        "optional": [
                            "caps_lock"
                        ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Finder.app'"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Open Chrome",
            "manipulators": [
                {
                    "from": {
                        "key_code": "c",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "osascript -e 'tell application \"Google Chrome\" to activate'"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
