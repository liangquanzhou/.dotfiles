{
    "title": "Launch Applications",
    "rules": [
        {
            "description": "Open Finder",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                        "optional": [
                            "caps_lock"
                        ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Finder.app'"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}

