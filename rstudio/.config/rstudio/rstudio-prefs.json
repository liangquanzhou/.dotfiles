{
    "editor_keybindings": "vim",
    "check_arguments_to_r_function_calls": true,
    "strip_trailing_whitespace": true,
    "soft_wrap_r_files": true,
    "syntax_color_console": true,
    "scroll_past_end_of_document": true,
    "save_files_before_build": true,
    "default_encoding": "UTF-8",
    "default_project_location": "~",
    "default_sweave_engine": "knitr",
    "document_author": "Liangquan Zhou",
    "show_hidden_files": true,
    "wrap_tab_navigation": false,
    "save_workspace": "never",
    "reuse_sessions_for_project_links": true,
    "jobs_tab_visibility": "shown",
    "posix_terminal_shell": "custom",
    "default_latex_program": "XeLaTeX",
    "use_tinytex": true,
    "rmd_viewer_type": "pane",
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "History",
            "Files",
            "Plots",
            "Connections",
            "Packages",
            "Help",
            "Build",
            "VCS",
            "Tutorial",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "highlight_r_function_calls": true,
    "num_spaces_for_tab": 4,
    "initial_working_directory": "~",
    "load_workspace": false,
    "show_last_dot_value": true,
    "remove_history_duplicates": true,
    "rainbow_parentheses": true,
    "check_unexpected_assignment_in_function_call": true,
    "warn_if_no_such_variable_in_scope": true,
    "warn_variable_defined_but_not_used": true,
    "style_diagnostics": true,
    "show_diagnostics_other": true,
    "check_for_updates": false,
    "show_panel_focus_rectangle": true,
    "full_project_path_in_window_title": true,
    "custom_shell_command": "/opt/homebrew/bin/zsh",
    "source_with_echo": true,
    "python_type": "system",
    "python_version": "3.11.7",
    "python_path": "/opt/homebrew/bin/python3",
    "git_exe_path": "/opt/homebrew/bin/git",
    "auto_detect_indentation": true,
    "auto_append_newline": true,
    "cran_mirror": {
        "name": "Global (CDN)",
        "host": "RStudio",
        "url": "https://cran.rstudio.com/",
        "country": "us",
        "ok": 1,
        "secondary": ""
    },
    "svn_exe_path": "/opt/homebrew/bin/svn",
    "highlight_selected_line": true,
    "relative_line_numbers": true,
    "blinking_cursor": false
}